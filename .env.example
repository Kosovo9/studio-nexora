# ===================================
# NEXORA SAFE-SHIP ENVIRONMENT CONFIG
# ===================================

# NextAuth Configuration
NEXTAUTH_SECRET=your-nextauth-secret-here
NEXTAUTH_URL=http://localhost:3000

# Admin Configuration
ADMIN_EMAIL=admin@nexora.com

# Database
DATABASE_URL=postgresql://username:password@localhost:5432/studio_nexora

# Stripe Configuration (Test Mode)
STRIPE_SECRET_KEY=sk_test_your_stripe_secret_key
STRIPE_PUBLISHABLE_KEY=pk_test_your_stripe_publishable_key
STRIPE_WEBHOOK_SECRET=whsec_your_webhook_secret

# Stripe Product Price IDs (Test Mode)
NEXT_PUBLIC_STRIPE_PRICE_BASIC=price_basic_test_id
NEXT_PUBLIC_STRIPE_PRICE_PRO=price_pro_test_id
NEXT_PUBLIC_STRIPE_PRICE_VIP=price_vip_test_id

# Storage Configuration (R2/S3)
STORAGE_BUCKET=nexora-uploads
STORAGE_ENDPOINT=https://your-account.r2.cloudflarestorage.com
STORAGE_KEY=your_storage_access_key
STORAGE_SECRET=your_storage_secret_key
STORAGE_REGION=auto

# Redis Configuration (for job queues)
REDIS_URL=redis://localhost:6379

# Replicate AI
REPLICATE_API_TOKEN=r8_your_replicate_token

# Cloudflare R2 (Alternative to S3)
CLOUDFLARE_ACCOUNT_ID=your_cloudflare_account_id
CLOUDFLARE_ACCESS_KEY_ID=your_r2_access_key
CLOUDFLARE_SECRET_ACCESS_KEY=your_r2_secret_key

# Rate Limiting
RATE_LIMIT_UPLOAD=10
RATE_LIMIT_CHECKOUT=5

# Feature Flags
ENABLE_GOOGLE_AUTH=true
ENABLE_MFA=false
ENABLE_ADMIN_DASHBOARD=true

# Monitoring
VERCEL_ANALYTICS_ID=your_vercel_analytics_id
